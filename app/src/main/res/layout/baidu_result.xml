<?xml version="1.0" encoding="utf-8"?>
<com.maple.rubbishseparator.view.PercentLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="horizontal"
    android:layout_marginTop="15dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/show_baidu_result_list_name"
        android:singleLine="true"
        android:ellipsize="end"
        android:layout_width="0dp"
        app:layout_widthPercent="70%"
        android:layout_height="wrap_content"
        android:textSize="25sp"
        android:textColor="@color/stringcolor_black"
        android:layout_marginStart="10dp"/>
    <TextView
        android:id="@+id/show_baidu_result_list_match"
        android:singleLine="true"
        android:ellipsize="end"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="25sp"
        android:gravity="right"
        android:textColor="@color/stringcolor_blue"
        android:layout_marginEnd="10dp"
        tools:ignore="RtlHardcoded" />


</com.maple.rubbishseparator.view.PercentLinearLayout>